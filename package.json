{
  "name": "ikid",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .eslintignore",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --ignore-path .eslintignore",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "generate-icons": "node scripts/generate-icons.js",
    "check-secrets": "node scripts/check-secrets.js all",
    "check-secrets-staged": "node scripts/check-secrets.js staged",
    "pre-commit": "npm run check-secrets-staged",
    "pre-push": "npm run check-secrets all"
  },
  "keywords": [
    "react",
    "webapp",
    "barnehage",
    "ikid"
  ],
  "author": "",
  "license": "ISC",
  "description": "iKid - Digital inn- og utkryssing for barnehager",
  "dependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.0",
    "firebase": "^12.5.0",
    "i18next": "^25.6.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.2.4",
    "react-router-dom": "^7.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.6.2",
    "sharp": "^0.34.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vite-plugin-pwa": "^1.1.0"
  }
}
